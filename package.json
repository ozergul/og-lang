{
  "name": "oglang",
  "version": "1.0.0",
  "description": "OgLang - A Statically Typed Programming Language",
  "type": "module",
  "scripts": {
    "run-calculator": "node runner.js examples/calculatorClass.oglang",
    "run-fibonacci": "node runner.js examples/fibonacci.oglang",
    "run-string-utils": "node runner.js examples/stringUtils.oglang",
    "run-array-operations": "node runner.js examples/arrayOperations.oglang",
    "run-math-operations": "node runner.js examples/mathOperations.oglang",
    "run-text-processing": "node runner.js examples/textProcessing.oglang",
    "run-functions": "node runner.js examples/functions.oglang",
    "run-examples": "make run-examples",
    "clean": "make clean",
    "test": "node tests/codeGenerator.test.js",
    "playground:install": "cd playground && pnpm install",
    "playground:start": "cd playground && pnpm start",
    "playground": "pnpm run playground:install && pnpm run playground:start"
  },
  "keywords": [
    "programming-language",
    "compiler",
    "static-typing",
    "javascript",
    "ast",
    "parser",
    "lexer"
  ],
  "author": "Özer Gül",
